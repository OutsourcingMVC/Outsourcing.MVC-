<script type="text/javascript">
    function nowYear() {
        var year = new Date().getFullYear();
        $("#nowyear").html(year);
    }
    nowYear();
    function set() {
        var loginid = $("#loginid").val();
        var type = $("#types").val();
        var hyzx = $("#hyzx");
        var dl = $("#btn-login-show");
        var zhuce = $("#zhuce");
        var krcgl = $("#krcgl");
        var k = $("#zpgl");
        var kzpgl = $("#kzpgl");
        var krcgl = $("#krcgl");
        var kxxfb = $("#kxxfb");
        var wrcgl = $("#wrcgl");
        if (loginid && loginid != "") {
            dl.hide();
            zhuce.hide();
            hyzx.show();
        }
        else {
            hyzx.hide();
        }
        //debugger;
        if (type && type != "") {
            if (type == "1") {
                kzpgl.hide(); krcgl.hide(); wrcgl.show(); //kxxfb.hide();
            }
            else {
                kzpgl.show(); krcgl.show(); wrcgl.hide(); //kxxfb.show();
            }
        }
    }
    set();

    //退出登录
    function Quit() {
        //debugger;
        $.ajax({
            type: "POST",
            url: "/System/Quit",
            data: "",
            success: function (msg) {
                //debugger;
                swal({
                    title: "退出登录",
                    text: "您确定要退出登录吗？",
                    type: "warning",
                    showCancelButton: true,
                    closeOnConfirm: false,
                    confirmButtonText: "确认",
                    confirmButtonColor: "#ec6c62"
                }, function () {
                    window.location.href = '/Home/Index';
                });
            }
        });
    }
</script>
