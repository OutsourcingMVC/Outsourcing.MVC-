    <style type="text/css">
        #navmenu-h {
            margin-top: 70px;
            margin-left: 25px;
        }

        #navmenu-h li {
            float: left;
            display: block;
            position: relative;
            height: 40px;
        }

        #navmenu-h ul {
            margin: 0;
            border: 0 none;
            padding: 0;
            list-style: none;
            display: none;
            position: absolute;
            top: 40px;
        }


        #navmenu-h ul li {
            float: right;
            display: block;
            margin-right: 50px;
        }

        #navmenu-h ul a {
            font-size: 26px;
            font-family: Arial, Helvetica, sans-serif;
            font-size: 24px;
            float: left;
            border-left: 30px;
            color: #666;
            text-decoration: none;
            height: 40px;
        }

        #aid {
            font-family: Arial, Helvetica, sans-serif;
            font-size: 28px;
            float: left;
            color: #666;
            height: 40px;
            padding-left: 15px;
        }

        #navmenu-h a:hover,
        #navmenu-h li:hover a,
        #navmenu-h li.iehover a {
            background: #9966CC;
            color: red;
            width: 160px;
        }


        #navmenu-h li:hover li a,
        #navmenu-h li.iehover li a {
            float: none;
            background: #EEE;
            color: #666;
        }

        #navmenu-h li:hover li a:hover,
        #navmenu-h li:hover li:hover a,
        #navmenu-h li.iehover li a:hover,
        #navmenu-h li.iehover li.iehover a {
            background: #CCC;
            color: #FFF;
        }

        #navmenu-h li:hover ul,
        #navmenu-h ul li:hover ul,
        #navmenu-h ul ul li:hover ul,
        #navmenu-h li.iehover ul,
        #navmenu-h ul li.iehover ul,
        #navmenu-h ul ul li.iehover ul {
            display: block;
        }

        #navpage a {
            color: black;
        }

        #navpage a:hover {
            color: green;
        }
    </style>
