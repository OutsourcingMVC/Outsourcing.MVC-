
@model Outsourcing.EF.Model.Recruit.MySelfModel
@{
    ViewBag.Title = "招聘中心";
    Layout = "~/Views/Shared/_RecruitLayout.cshtml";
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>CreateRecruit</title>
    <script type="text/javascript" src="~/Scripts/calendar/js/lyz.calendar.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <link href="~/Scripts/calendar/css/lyz.calendar.css" rel="stylesheet" />
    <!--消息提示框-->
    <link rel="stylesheet" href="~/Scripts/sweetalert/sweetalert.css" />
    <script type="text/javascript" src="~/Scripts/sweetalert/sweetalert.min.js"></script>
    <!-- 编辑器样式 -->
    <script type="text/javascript" charset="utf-8" src="~/Scripts/ueditor/utf8-net/ueditor.config.js"></script>
    <script type="text/javascript" charset="utf-8" src="~/Scripts/ueditor/utf8-net/ueditor.all.min.js"> </script>
    <!--建议手动加在语言，避免在ie下有时因为加载语言失败导致编辑器加载失败-->
    <!--这里加载的语言文件会覆盖你在配置项目里添加的语言类型，比如你在配置项目里配置的是英文，这里加载的中文，那最后就是中文-->
    <script type="text/javascript" charset="utf-8" src="~/Scripts/ueditor/utf8-net/lang/zh-cn/zh-cn.js"></script>
    <link href="~/Scripts/styles/extends/ViewCss/recruitCenter.css" rel="stylesheet" />
    <style type="text/css">
        #td tr {
            line-height: 30px;
        }

        #td a:hover {
            color: red;
            font-weight: 800;
        }

        #tb_1 tr {
            line-height: 40px;
        }

        #tb_1 input {
            width: 200px;
            height: 20px;
        }

        #tb_1 span {
            color: red;
        }

        .btn {
            color: #606060;
            border: solid 1px #b7b7b7;
            background: #fff;
            background: -webkit-gradient(linear, left top, left bottom, from(#fff), to(#ededed));
            background: -moz-linear-gradient(top, #fff, #ededed);
            filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ffffff', endColorstr='#ededed');
            font-size: 14px;
            font-weight: 500;
        }

            .btn:hover {
                background: #ededed;
                background: -webkit-gradient(linear, left top, left bottom, from(#fff), to(#dcdcdc));
                background: -moz-linear-gradient(top, #fff, #dcdcdc);
                filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ffffff', endColorstr='#dcdcdc');
            }

            .btn:active {
                color: #999;
                background: -webkit-gradient(linear, left top, left bottom, from(#ededed), to(#fff));
                background: -moz-linear-gradient(top, #ededed, #fff);
                filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ededed', endColorstr='#ffffff');
            }
    </style>
</head>
<body>
    <input type="hidden" value="@ViewBag.LoginID" />
    <div style="margin-left:70px;margin-top:10px;">
        <div style="width:10%;float:left;font-size:14px;border:1px solid rgb(248, 242, 242)">
            <table style="width:100%;" id="td">
                <tr>
                    <td style="text-align:left"><strong style="font-size:14px;padding-left:10px;">内容</strong></td>
                </tr>
                <tr>
                    <td style="text-align:center"><a href="javascript:void()" id="onself" url="/Recruit/Recruit/CreateMyself">个人信息</a></td>
                </tr>
                <tr>
                    <td style="text-align:center"><a href="javascript:void()" id="myRated" url="/Recruit/Recruit/CreateMyrated">自我评价</a></td>
                </tr>
                <tr>
                    <td style="text-align:center"><a href="javascript:void()" id="myExpectations" url="/Recruit/Recruit/CreateMyExpectations">求职意向</a></td>
                </tr>
                <tr>
                    <td style="text-align:center"><a href="javascript:void()" id="myWorkexperience" url="/Recruit/Recruit/CreateMyWorkexperience">工作经验</a></td>
                </tr>
                <tr>
                    <td style="text-align:center"><a href="javascript:void()" id="myEducation" url="/Recruit/Recruit/MyEducation ">教育背景</a></td>
                </tr>
                <tr>
                    <td style="text-align:center"><a href="javascript:void()" id="myTrain" url="/Recruit/Recruit/MyTrain">培训经历</a></td>
                </tr>
                <tr>
                    <td style="text-align:center"><a href="javascript:void()" id="myLanguage" url="/Recruit/Recruit/MyLanguage">语言能力</a></td>
                </tr>
                <tr>
                    <td style="text-align:center"><a href="javascript:void()" id="myCertificate" url="/Recruit/Recruit/MyCertificate">证书</a></td>
                </tr>
                <tr>
                    <td style="text-align:center">照片/附件</td>
                </tr>
                <tr>
                    <td style="text-align:center"><a href="javascript:void()" id="myElseinfo" url="/Recruit/Recruit/MyElseinfo">其他信息</a></td>
                </tr>
                <tr>
                    <td style="text-align:left"><strong style="font-size:14px;padding-left:10px;">人才附加内容</strong></td>
                </tr>
                <tr>
                    <td style="text-align:center"><a href="javascript:void()" id="myProject" url="/Recruit/Recruit/MyProject">项目经验</a></td>
                </tr>
                <tr>
                    <td style="text-align:center"><a href="javascript:void()" id="myMajor" url="/Recruit/Recruit/MyMajor">专业技能</a></td>
                </tr>
                <tr>
                    <td style="text-align:center"><a href="javascript:void()" id="MySchoolStudy" url="/Recruit/Recruit/MySchoolStudy">在校学习情况</a></td>
                </tr>
                <tr>
                    <td style="text-align:center"><a href="javascript:void()" id="MyPractice" url="/Recruit/Recruit/MyPractice">在校实践情况</a></td>
                </tr>
            </table>
        </div>
        <div style="width:82%;float:left;margin-left:20px;border:1px solid rgb(248, 242, 242);" id="main">
            <div class="panel" style="height:40px;border-bottom:1px solid blue;width:100%;">
                <span style="font-size:18px;margin-top:50px;margin-left:10px;font-weight:800;">个人信息</span>
            </div>
            <div>
                @using (Html.BeginForm("", "", FormMethod.Post, new { id = "myself" }))
                {
                    @Html.ValidationSummary(true);
                    <table style="width:100%;margin-top:10px;font-size:12px;" id="tb_1">
                        <tr>
                            <td style="text-align:right; width:40%;"><span style="color:red;">*&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>简历名称：</td>
                            <td style="text-align:left;">
                                @Html.TextBoxFor(Model => Model.RecruitName)
                                @Html.ValidationMessageFor(Model => Model.RecruitName)
                            </td>
                        </tr>
                        <tr>
                            <td style="text-align:right;"><span style="color:red;">*&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>姓名：</td>
                            <td style="text-align:left;">

                                @Html.TextBoxFor(Model => Model.UserName)
                                @Html.ValidationMessageFor(Model => Model.UserName)
                            </td>
                        </tr>
                        <tr>
                            <td style="text-align:right"><span style="color:red;">*&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>性别：</td>

                            @{
                                if (Model.UserSex == "男")
                                {
                                    <td style="text-align:left">
                                        @Html.RadioButtonFor(Model => Model.UserSex, "男", new { @checked = true, style = "width:15px;" }) &nbsp;&nbsp; 男
                                        @Html.RadioButtonFor(Model => Model.UserSex, "女", new { style = "width:15px;" }) &nbsp;&nbsp;女
                                    </td>
                                }
                                else
                                {
                                    <td style="text-align:left;width:100px;">
                                        @Html.RadioButtonFor(Model => Model.UserSex, "男", new { style = "width:15px;" }) &nbsp;&nbsp; 男
                                        @Html.RadioButtonFor(Model => Model.UserSex, "女", new { @checked = false, style = "width:15px;" }) &nbsp;&nbsp;女
                                    </td>
                                }
                            }
                        </tr>
                        <tr>
                            <td style="text-align:right"><span style="color:red;">*&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>出生日期：</td>
                            <td style="text-align:left;">

                                @Html.TextBoxFor(Model => Model.Birthday)
                            </td>
                        </tr>
                        <tr>
                            <td style="text-align:right"><span style="color:red;">*&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>工作月份：</td>
                            <td style="text-align:left;">

                                @Html.TextBoxFor(Model => Model.AttendYear)
                            </td>
                        </tr>
                        <tr>
                            <td style="text-align:right"><span style="color:red;">*&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>户口所在地：</td>
                            <td style="text-align:left;">

                                @Html.TextBoxFor(Model => Model.RegisteredPerson)
                            </td>
                        </tr>
                        <tr>
                            <td style="text-align:right"><span style="color:red;">*&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>现居地：</td>
                            <td style="text-align:left;">

                                @Html.TextBoxFor(Model => Model.NowAdress)
                            </td>
                        </tr>
                        <tr>
                            <td style="text-align:right"><span style="color:red;">*&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>联系方式：</td>
                            <td style="text-align:left;">

                                @Html.TextBoxFor(Model => Model.TelPhone)
                            </td>
                        </tr>
                        <tr>
                            <td style="text-align:right"><span style="color:red;">*&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>电子邮箱：</td>
                            <td style="text-align:left;">

                                @Html.TextBoxFor(Model => Model.Mail)
                            </td>
                        </tr>
                        <tr>
                            <td style="text-align:right"><span style="color:red;">*&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>国籍：</td>
                            <td style="text-align:left;">

                                @Html.TextBoxFor(Model => Model.Nationality)
                            </td>
                        </tr>
                        <tr>
                            <td style="text-align:right"><span style="color:red;">*&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>证件：</td>
                            <td style="text-align:left;">

                                @Html.TextBoxFor(Model => Model.Nationality)
                                @Html.TextBoxFor(model => model.CredentialsNumber)
                            </td>
                        </tr>
                        <tr>
                            <td style="text-align:right"><span style="color:red;">*&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>海外工作/学习经历：</td>

                            @{
                                if (Model.UserSex == "男")
                                {
                                    <td style="text-align:left">
                                        @Html.RadioButtonFor(Model => Model.OverseasStudent, "是", new { @checked = true, style = "width:15px;" }) &nbsp;&nbsp; 是
                                        @Html.RadioButtonFor(Model => Model.OverseasStudent, "否", new { style = "width:15px;" }) &nbsp;&nbsp;否
                                    </td>
                                }
                                else
                                {
                                    <td style="text-align:left;width:100px;">
                                        @Html.RadioButtonFor(Model => Model.OverseasStudent, "是", new { style = "width:15px;" }) &nbsp;&nbsp; 是
                                        @Html.RadioButtonFor(Model => Model.OverseasStudent, "否", new { @checked = false, style = "width:15px;" }) &nbsp;&nbsp;否
                                    </td>
                                }
                            }
                        </tr>
                        <tr>
                            <td style="text-align:right"><span style="color:red;">*&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;</span>政治面貌：</td>
                            @{
                                if (Model.PoliticsStatus == "中共党员")
                                {
                                    <td style="text-align:left;width:200px;">
                                        @Html.RadioButtonFor(Model => Model.PoliticsStatus, "中共党员", new { @checked = true, style = "width:15px;" }) &nbsp;&nbsp;中共党员
                                        @Html.RadioButtonFor(Model => Model.PoliticsStatus, "团员", new { style = "width:15px;" })       &nbsp;&nbsp;&nbsp;团员
                                        @Html.RadioButtonFor(Model => Model.PoliticsStatus, "群众", new { style = "width:15px;" })       &nbsp;&nbsp;&nbsp;群众
                                        @Html.RadioButtonFor(Model => Model.PoliticsStatus, "民主党派", new { style = "width:15px;" })   &nbsp;&nbsp;&nbsp;民主党派
                                        @Html.RadioButtonFor(Model => Model.PoliticsStatus, "无党派人士", new { style = "width:15px;" }) &nbsp;&nbsp;&nbsp;无党派人士
                                    </td>
                                }
                                else if (Model.PoliticsStatus == "团员")
                                {
                                    <td style="text-align:left;width:200px;">
                                        @Html.RadioButtonFor(Model => Model.PoliticsStatus, "中共党员", new { style = "width:15px;" }) &nbsp;&nbsp;中共党员
                                        @Html.RadioButtonFor(Model => Model.PoliticsStatus, "团员", new { @checked = true, style = "width:15px;" })&nbsp;&nbsp;&nbsp;团员
                                        @Html.RadioButtonFor(Model => Model.PoliticsStatus, "群众", new { style = "width:15px;" })                         &nbsp;&nbsp;&nbsp;群众
                                        @Html.RadioButtonFor(Model => Model.PoliticsStatus, "民主党派", new { style = "width:15px;" })                     &nbsp;&nbsp;&nbsp;民主党派
                                        @Html.RadioButtonFor(Model => Model.PoliticsStatus, "无党派人士", new { style = "width:15px;" })                   &nbsp;&nbsp;&nbsp;无党派人士
                                    </td>
                                }
                                else if (Model.PoliticsStatus == "群众")
                                {
                                    <td style="text-align:left;width:200px;">
                                        @Html.RadioButtonFor(Model => Model.PoliticsStatus, "中共党员", new { style = "width:15px;" })&nbsp;&nbsp;中共党员
                                        @Html.RadioButtonFor(Model => Model.PoliticsStatus, "团员", new { style = "width:15px;" })                            &nbsp;&nbsp;&nbsp;团员
                                        @Html.RadioButtonFor(Model => Model.PoliticsStatus, "群众", new { @checked = true, style = "width:15px;" })   &nbsp;&nbsp;&nbsp;群众
                                        @Html.RadioButtonFor(Model => Model.PoliticsStatus, "民主党派", new { style = "width:15px;" })                        &nbsp;&nbsp;&nbsp;民主党派
                                        @Html.RadioButtonFor(Model => Model.PoliticsStatus, "无党派人士", new { style = "width:15px;" })                      &nbsp;&nbsp;&nbsp;无党派人士
                                    </td>
                                }
                                else if (Model.PoliticsStatus == "民主党派")
                                {
                                    <td style="text-align:left;width:200px;">
                                        @Html.RadioButtonFor(Model => Model.PoliticsStatus, "中共党员", new { style = "width:15px;" })&nbsp;&nbsp;中共党员
                                        @Html.RadioButtonFor(Model => Model.PoliticsStatus, "团员", new { style = "width:15px;" })                              &nbsp;&nbsp;&nbsp;团员
                                        @Html.RadioButtonFor(Model => Model.PoliticsStatus, "群众", new { style = "width:15px;" })                              &nbsp;&nbsp;&nbsp;群众
                                        @Html.RadioButtonFor(Model => Model.PoliticsStatus, "民主党派", new { @checked = true, style = "width:15px;" }) &nbsp;&nbsp;&nbsp;民主党派
                                        @Html.RadioButtonFor(Model => Model.PoliticsStatus, "无党派人士", new { style = "width:15px;" })                        &nbsp;&nbsp;&nbsp;无党派人士
                                    </td>
                                }
                                else
                                {
                                    <td style="text-align:left;width:200px;">
                                        @Html.RadioButtonFor(Model => Model.PoliticsStatus, "中共党员", new { style = "width:15px;" })&nbsp;&nbsp;中共党员
                                        @Html.RadioButtonFor(Model => Model.PoliticsStatus, "团员", new { style = "width:15px;" })                               &nbsp;&nbsp;&nbsp;团员
                                        @Html.RadioButtonFor(Model => Model.PoliticsStatus, "群众", new { style = "width:15px;" })                               &nbsp;&nbsp;&nbsp;群众
                                        @Html.RadioButtonFor(Model => Model.PoliticsStatus, "民主党派", new { style = "width:15px;" })                           &nbsp;&nbsp;&nbsp;民主党派
                                        @Html.RadioButtonFor(Model => Model.PoliticsStatus, "无党派人士", new { @checked = true, style = "width:15px;" })&nbsp;&nbsp;&nbsp;无党派人士
                                    </td>
                                }

                            }
                        </tr>
                    </table>
                    <input type="submit" value="提交" class="btn" style="width:100px;height:40px;margin-left:500px;margin-top:40px;" />
                                }

            </div>
            <div style="height:30px;"></div>
        </div>
    </div>
</body>
</html>

<script type="text/javascript">
    $("table tr td a").click(function () {
        var url = $(this).attr("url");
        $.ajax({
            url: url,
            data: {"loginid":@ViewBag.LoginID},
            type:"POST",
            dateType:"HTML",
            success:function(RDate){

            }
        })
    })

</script>

