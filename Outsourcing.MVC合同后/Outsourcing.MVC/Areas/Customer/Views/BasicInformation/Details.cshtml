@model Outsourcing.EF.CustomerCompnay
@{
    ViewBag.Title = "Details";
}

<div class="row">
    <div id="breadcrumb" class="col-xs-12">
        <a href="#" class="show-sidebar">
            <i class="fa fa-bars"></i>
        </a> 
        <ol class="breadcrumb pull-left">
            <li><a onclick="LoadAjaxContent('Customer/BasicInformation/Index?page=@ViewBag.pageIndex&companyname=@HttpUtility.UrlEncodeUnicode(ViewBag.SearchString)')" href="#">返回列表页</a></li>
            @*<li><a href="#">Tables</a></li>
                <li><a href="#">Data Tables</a></li>*@
        </ol>
        @*<div id="social" class="pull-right">
            <a href="#"><i class="fa fa-google-plus"></i></a>
            <a href="#"><i class="fa fa-facebook"></i></a>
            <a href="#"><i class="fa fa-twitter"></i></a>
            <a href="#"><i class="fa fa-linkedin"></i></a>
            <a href="#"><i class="fa fa-youtube"></i></a>
        </div>*@
    </div>
</div>
<div class="row">
    <div class="col-xs-12">
        <div class="box">
            <div class="box-header">
                <div class="box-name">
                    <i class="fa fa-linux"></i>
                    <span>查看客户公司基本资料</span>
                </div>
                <div class="box-icons">
                    <a class="collapse-link">
                        <i class="fa fa-chevron-up"></i>
                    </a>
                    <a class="expand-link">
                        <i class="fa fa-expand"></i>
                    </a>
                    <a class="close-link">
                        <i class="fa fa-times"></i>
                    </a>
                </div>
                <div class="no-move"></div>
            </div>
            <div class="box-content no-padding">
                <form id="defaultForm" method="post"  class="form-horizontal">
                    <fieldset>
                        @{
                            if (Model != null)
                            {
                                <div class="col-sm-4 control-label" style="background-color:#ffffff;">
                                    <div class="form-group">
                                        <label class="col-sm-4 control-label" style="color:#ff0000">公司名称</label>
                                        <div class="col-sm-7">
                                            @Html.Hidden("outsourcingcompanyId", @Model.CompnayId)
                                            <label class="control-label">@Model.CompnayName</label>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-4 control-label" style="color:#ff0000">用户名</label>
                                        <div class="col-sm-7">
                                            <label class="control-label">@Model.CompanyUserName</label>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-4 control-label">密码</label>
                                        <div class="col-sm-7">
                                            <label class="control-label">@Model.Pwd</label>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-4 control-label">英文名称</label>
                                        <div class="col-sm-7">
                                            <label class="control-label">@Model.EnglishName</label>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-4 control-label">企业法人</label>
                                        <div class="col-sm-7">
                                            <label class="control-label">@Model.LegalRepresentative</label>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-4 control-label">法人联系电话</label>
                                        <div class="col-sm-7">
                                            <label class="control-label">@Model.LegalTelephone</label>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-4 control-label">单位负责人</label>
                                        <div class="col-sm-7">
                                            <label class="control-label">@Model.UnitResponsible</label>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-4 control-label">负责人电话</label>
                                        <div class="col-sm-7">
                                            <label class="control-label">@Model.ResponsibleTelephone</label>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-4 control-label">公司性质</label>
                                        <div class="col-sm-7">
                                            <label class="control-label">@Model.Nature</label>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-4 control-label">公司地址</label>
                                        <div class="col-sm-7">
                                            <label class="control-label">@Model.Address</label>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-4 control-label">公司营业证书</label>
                                        <div class="col-sm-7">
                                            <label class="control-label">@Model.BusinessCertificate</label>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-4 control-label">营业期限</label>
                                        <div class="col-sm-7">
                                            <label class="control-label">@Model.OperatingPeriod</label>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-4 control-label">登记机关</label>
                                        <div class="col-sm-7">
                                            <label class="control-label">@Model.RegistrationAuthority</label>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-4 control-label">经营范围</label>
                                        <div class="col-sm-7">
                                            <label class="control-label">@Model.BusinessScope</label>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-4 control-label">类型</label>
                                        <div class="col-sm-7">
                                            <label class="control-label">@Model.Type</label>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-4 control-label">注册号</label>
                                        <div class="col-sm-7">
                                            <label class="control-label">@Model.EnterpriseNum</label>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-4 control-label">联系电话1</label>
                                        <div class="col-sm-7">
                                            <label class="control-label">@Model.Telephone1</label>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-4 control-label">联系电话2</label>
                                        <div class="col-sm-7">
                                            <label class="control-label">@Model.Telephone2</label>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-4 control-label">合同资料</label>
                                        <div class="col-sm-7">
                                            @*<div class="bjceng">
                    <img src="@Model.StaffMember.ContactFile"/>
                </div>*@
                                            <a id="@Model.Contract" href="/res/web/viewer.html?file=@Model.Contract" class="contactfile">浏览</a>
                                            @*<a class="de" href="@Model.StaffMember.ContactFile">合同浏览</a>*@
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-4 control-label">审核状态</label>
                                        <div class="col-sm-7">
                                            <label class="control-label">@Model.AuditingStatue</label>
                                        </div>
                                    </div>
                                </div>
                            }
                        }
                    </fieldset>
                </form>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript">
    $(document).ready(function () {

        // Add Drag-n-Drop feature
        WinMove();
    });
</script>

